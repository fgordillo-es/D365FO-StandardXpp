			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        super();
        
        QueryBuildDataSource workflowWorkItemTableDataSource = WorkflowWorkItemTable_ds.query().dataSourceTable(tableNum(WorkflowWorkItemTable));
        workflowWorkItemTableDataSource.clearDynalinks();
        workflowWorkItemTableDataSource.addDynalink(fieldNum(WorkflowWorkItemTable, RefRecId), WorkflowWorkItemTable, fieldNum(VendInvoiceInfoTable, RecId));
        QueryBuildRange workflowWorkItemTableRange = workflowWorkItemTableDataSource.addRange(fieldNum(WorkflowWorkItemTable, RefTableId));
        workflowWorkItemTableRange.value(int2Str(tableNum(VendInvoiceInfoTable)));
        workflowWorkItemTableRange.status(RangeStatus::Hidden);
    }

]]></Source>
			</Method>
		</Methods>
		<DataSources xmlns="">
			<DataSource>
				<Name>UserInfo</Name>
				<Methods>
					<Method>
						<Name>executeQuery</Name>
						<Source><![CDATA[
        public void executeQuery()
        {
            this.query().dataSourceTable(tableNum(UserInfo)).clearDynalinks();

            WorkflowWorkItemTable workflowWorkItemTableLocal;
            str value;

            value = strFmt('(%1.%2 != "%3")', userInfo_ds.name(), fieldStr(UserInfo, Id), curUserId());
            
            while select UserId
                from workflowWorkItemTableLocal
                    where workflowWorkItemTableLocal.CorrelationId == workflowWorkItemTable.CorrelationId &&
                        workflowWorkItemTableLocal.StepId == workflowWorkItemTable.StepId &&
                        workflowWorkItemTableLocal.Status == WorkflowWorkItemStatus::Pending
            {
                if (value)
                {
                    value += ' && ';
                }
                value += strFmt('(%1.%2 != "%3")', userInfo_ds.name(), fieldStr(UserInfo, Id), workflowWorkItemTable.UserId);
            }

            QueryBuildRange userIdRange = this.query().dataSourceTable(tableNum(UserInfo)).addRange(fieldNum(UserInfo, Id));
            userIdRange.value(value);

            super();
        }

]]></Source>
					</Method>
