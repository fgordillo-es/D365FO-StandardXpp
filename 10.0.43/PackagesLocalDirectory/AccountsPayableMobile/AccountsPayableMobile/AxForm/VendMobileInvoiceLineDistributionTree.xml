				<Methods>
					<Method>
						<Name>init</Name>
						<Source><![CDATA[
        public void init()
        {
            super();

            // Ensure that this form opens empty and doesn't just grab the first <c>VendInvoiceInfoTable</c> record. Depending on the size of the invoice
            // the opening of the form and running the query can be expensive and for mobile, it opens, runs the code once, and then applies the correct
            // record filter after that.  This code prevents us from unnecessarily running our code twice.
            VendInvoiceInfoLine_ds.query().dataSourceTable(tableNum(VendInvoiceInfoLine)).addRange(fieldNum(VendInvoiceInfoLine, RecId)).value(queryValue(0));
        }

]]></Source>
					</Method>
					<Method>
						<Name>executeQuery</Name>
						<Source><![CDATA[
        public void executeQuery()
        {
            super();

            VendMobileInvoiceDistributionTree distributionTree;

            boolean processParents = true;

            if(element.args().parmEnumType() == enumNum(VendMobileAccountingDistributionDisplayMode) &&
               element.args().parmEnum() == VendMobileAccountingDistributionDisplayMode::AllLinesLeafNodesOnly)
            {
                processParents = false;
            }

            SourceDocumentLine sourceDocumentLine = SourceDocumentLine::find(VendInvoiceInfoLine.SourceDocumentLine);
            VendMobileInvoiceDistributionTree::populateTempBuffer(distributionTree, sourceDocumentLine.SourceDocumentHeader, vendInvoiceInfoLine.SourceDocumentLine, processParents);
            vendMobileInvoiceDistributionTree.setTmpData(distributionTree);
            vendMobileInvoiceDistributionTree_ds.executeQuery();
        }

]]></Source>
					</Method>
