			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        super();

        // Ensure that this form opens empty and doesn't just grab the first <c>VendInvoiceInfoTable</c> record. Depending on the size of the invoice
        // the opening of the form and running the query can be expensive and for mobile, it opens, runs the code once, and then applies the correct
        // record filter after that.  This code prevents us from unnecessarily running our code twice.
        VendInvoiceInfoTable_ds.query().dataSourceTable(tableNum(VendInvoiceInfoTable)).addRange(fieldNum(VendInvoiceInfoTable, RecId)).value(queryValue(0));
    }

]]></Source>
			</Method>
		</Methods>
		<DataSources xmlns="">
			<DataSource>
				<Name>VendInvoiceInfoTable</Name>
				<Methods>
					<Method>
						<Name>executeQuery</Name>
						<Source><![CDATA[
        public void executeQuery()
        {
            super();

            VendMobileInvoiceDistributionTree distributionsTree;

            boolean processParents = true;
            TableId tableToSkip = 0;

            if(element.args().parmEnumType() == enumNum(VendMobileAccountingDistributionDisplayMode))
            {
                switch(element.args().parmEnum())
                {
                    case VendMobileAccountingDistributionDisplayMode::AllLinesLeafNodesOnly:
                        processParents = false;
                        break;
                    case VendMobileAccountingDistributionDisplayMode::HeaderOnly:
                        tableToSkip = tableNum(VendInvoiceInfoLine);
                        break;
                    case VendMobileAccountingDistributionDisplayMode::HeaderLeafNodesOnly:
                        processParents = false;
                        tableToSkip = tableNum(VendInvoiceInfoLine);
                        break;
                }
            }

            VendMobileInvoiceDistributionTree::populateTempBuffer(distributionsTree, VendInvoiceInfoTable.SourceDocumentHeader, 0, processParents, tableToSkip);

            vendMobileInvoiceDistributionTree.setTmpData(distributionsTree);
            vendMobileInvoiceDistributionTree_ds.executeQuery();
        }

]]></Source>
					</Method>
				</Methods>
